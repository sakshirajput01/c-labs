#include <stdio.h>
#include <math.h>
int main() {
    float x1,y1,x2,y2,dist;
    printf("Enter the coords of A (x1 y1): ");
    scanf("%f %f", &x1, &y1);
    printf("Enter the coords of B (x2 y2): ");
    scanf("%f %f", &x2, &y2);
    dist = sqrt(pow(x2 - x1, 2) + pow(y2 - y1, 2));
    printf("Distance b/w A & B: %.2f\n", dist);
    return 0;
}
